let opLessThanEq = func(x, y) {
    return x <= y
}

let opLessThan = func(x, y) {
    return x < y
}

let opGreaterThanEq = func(x, y) {
    return x >= y
}

let opGreaterThan = func(x, y) {
    return x > y
}

let opNotEqual = func(x, y) {
    return x != y
}

let opEqualEqual = func(x, y) {
    return x == y
}

let modTwoNumbers = func(x, y) {
    return x % y
}

let divTwoNumbers = func(x, y) {
    return x / y
}

let mulTwoNumbers = func(x, y) {
    return x * y
}

let addAndSub = func(x, y, z) {
    return x + y - z
}
